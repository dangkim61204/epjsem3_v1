@model Data_DAL.Entities.Client

@{
    ViewData["Title"] = "Client | Create ";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<h1>Create Client</h1>



<div class="row">
    <div class="col-xxl-12">
        <div class="card">
        

            <div class="card-body">

                <div class="live-preview">
                    <form asp-action="Create" >
                        <div class="row">
                            <div class="col-md-12">
                                <div class="mb-3">
                                    <label asp-for="Name" class="form-label"></label>
                                    <input asp-for="Name" type="text" class="form-control" placeholder="Enter your Name">
                                    <span asp-validation-for="Name" class="text-danger"></span>
                                </div>
                            </div>
 
                            <div class="col-md-12">
                                <div class="mb-3">
                                    <label asp-for="Phone" class="form-label"></label>
                                    <input asp-for="Phone" type="text" class="form-control" placeholder="+(83) 1451 45123">
                                    <span asp-validation-for="Phone" class="text-danger"></span>
                                </div>
                            </div>
                            <!--end col-->
                            <div class="col-md-12">
                                <div class="mb-3">
                                    <label asp-for="Email" class="form-label"></label>
                                    <input asp-for="Email" type="email" class="form-control" placeholder="example@gamil.com">
                                    <span asp-validation-for="Email" class="text-danger"></span>
                                </div>
                            </div>
                            <!--end col-->
                            <div class="col-md-12">
                                <div class="mb-3">
                                    <label asp-for="Address" class="form-label"></label>
                                    <input asp-for="Address" type="text" class="form-control" placeholder="Address ">
                                    <span asp-validation-for="Address" class="text-danger"></span>
                                </div>
                            </div>
                           
                          
                          
                             <div class="col-md-12">
                                 <div class="mb-3">
                                     <label asp-for="ServiceId" class="form-label"></label>

                                     <select asp-for="ServiceId" class="form-control" asp-items="ViewBag.serviceId"></select>
                                 </div>
                             </div>

                               <div class="col-md-12">
                                   <div class="mb-3">
                                    <label class="form-label">Emplyee select</label>

                                    <select name="emplyeeIds" class="form-control" asp-items="ViewBag.EmployeeSelectList" multiple="multiple"></select>
                                   </div>
                               </div>
                           
                            <div class="col-lg-12">
                                <div class="text-end">
                                    <button type="submit" class="btn btn-primary">Create</button>
                                </div>
                            </div>
                            <!--end col-->
                        </div>
                        <!--end row-->
                    </form>
                </div>
